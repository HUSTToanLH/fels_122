<div class="col-md-6 category">

  <ul class="no-decoration">
    <li>
      <ul class="list-inline">
        <li>
          <h3><strong><%= category.name %></strong></h3>
        </li>
      </ul>
    </li>
    <li>
      <div class="col-md-12 textbox text-justify">
        <%= category.description %>
      </div>
    </li>
    <li>
      <div class="btn btn-default col-md-3 col-md-offset-9">
        <%= link_to t("views.categories.start"), "#" %>
      </div>
    </li>
  </ul>
</div>
